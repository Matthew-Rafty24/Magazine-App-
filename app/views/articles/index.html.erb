<% @elephants.each do |article| %>
<tr>
	<td><%= article.id %></td>
	<td><%= link_to article.title, article_path(article) %></td>
	<td><%= article.created_at %></td>
	<td><%= link_to "Edit", edit_article_path(article) %></td>
	<td><%= link_to "Delete", article, method: :delete %></td>
</tr>
<% end %>
</table>

